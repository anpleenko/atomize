{
  "name": "atomize-fork",
  "displayName": "Atomize Fork (Atom One Dark theme)",
  "description": "A detailed and accurate Atom One Dark Theme",
  "version": "1.0.0",
  "publisher": "anpleenko",
  "icon": "images/icon.png",
  "author": {
    "name": "anpleenko"
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "color",
    "theme",
    "atom",
    "dark",
    "one dark"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Atomize fork",
        "uiTheme": "vs-dark",
        "path": "./themes/Atomize-color-theme.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#282C34",
    "theme": "dark"
  },
  "scripts": {
    "fix": "prettier '**/*.{json,md,yml}' --write",
    "lint": "prettier '**/*.{json,md,yml}' --check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "**/*.{json,md,yml}": "npm run fix"
  },
  "bugs": {
    "url": "https://github.com/emroussel/atomize/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emroussel/atomize.git"
  },
  "homepage": "https://github.com/emroussel/atomize/blob/master/README.md",
  "devDependencies": {
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5"
  }
}
